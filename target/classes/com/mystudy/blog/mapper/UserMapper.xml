<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mystudy.blog.mapper.UserMapper">
    <insert id="insertUser" parameterType="com.mystudy.blog.bean.User">
        insert into user_info(account_id,name,token,gmt_create,gmt_modified) values(#{accountId},#{name},#{token},#{gmtCreate},#{getModified})
    </insert>
</mapper>